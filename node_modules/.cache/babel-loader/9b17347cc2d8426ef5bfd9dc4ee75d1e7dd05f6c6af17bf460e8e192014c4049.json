{"ast":null,"code":"import { useStore } from 'vuex';\nexport default {\n  name: 'signin',\n  data() {\n    const store = useStore();\n    console.log(store.state.SingupEmail);\n    return {\n      store\n    };\n  },\n  methods: {\n    submitSiginup: function () {\n      const signup = {\n        email: this.store.state.SingupEmail,\n        password: this.store.state.SignupPassword\n      };\n      const SignedUp = JSON.parse(sessionStorage.getItem('authenticate'));\n      if (SignedUp) {\n        this.store.state.ShowErrorSignup = true;\n        this.store.state.ErrorSingUp = \"This Email Signed Up!\";\n      } else {\n        localStorage.setItem(\"authenticate\", JSON.stringify(signup));\n        sessionStorage.setItem(\"authenticate\", JSON.stringify(signup));\n        window.location.reload();\n      }\n    },\n    showpass: function () {\n      const inp = document.getElementById('password');\n      if (inp.type === \"password\") {\n        inp.type = \"text\";\n      } else {\n        inp.type = \"password\";\n      }\n      ;\n    }\n  }\n};","map":{"version":3,"mappings":"AAuCA,SAASA,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIH,QAAQ,EAAE;IAExBI,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,KAAK,CAACC,WAAW,CAAC;IAEpC,OAAO;MACHJ;IACJ;EACJ,CAAC;EACDK,OAAO,EAAE;IACLC,aAAa,EAAE,YAAY;MAEvB,MAAMC,MAAK,GAAI;QACXC,KAAK,EAAE,IAAI,CAACR,KAAK,CAACG,KAAK,CAACC,WAAW;QACnCK,QAAQ,EAAE,IAAI,CAACT,KAAK,CAACG,KAAK,CAACO;MAC/B,CAAC;MAED,MAAMC,QAAO,GAAIC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,cAAc,CAAC,CAAC;MAEnE,IAAIJ,QAAQ,EAAE;QAEV,IAAI,CAACX,KAAK,CAACG,KAAK,CAACa,eAAc,GAAI,IAAI;QAEvC,IAAI,CAAChB,KAAK,CAACG,KAAK,CAACc,WAAU,GAAI,uBAAuB;MAC1D,OAAO;QAEHC,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEP,IAAI,CAACQ,SAAS,CAACb,MAAM,CAAC,CAAC;QAE5DO,cAAc,CAACK,OAAO,CAAC,cAAc,EAAEP,IAAI,CAACQ,SAAS,CAACb,MAAM,CAAC,CAAC;QAE9Dc,MAAM,CAACC,QAAQ,CAACC,MAAM,EAAE;MAC5B;IACJ,CAAC;IACDC,QAAQ,EAAE,YAAY;MAClB,MAAMC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC;MAC/C,IAAIF,GAAG,CAACG,IAAG,KAAM,UAAU,EAAE;QACzBH,GAAG,CAACG,IAAG,GAAI,MAAM;MACrB,OACK;QACDH,GAAG,CAACG,IAAG,GAAI,UAAU;MACzB;MAAC;IACL;EACJ;AACJ","names":["useStore","name","data","store","console","log","state","SingupEmail","methods","submitSiginup","signup","email","password","SignupPassword","SignedUp","JSON","parse","sessionStorage","getItem","ShowErrorSignup","ErrorSingUp","localStorage","setItem","stringify","window","location","reload","showpass","inp","document","getElementById","type"],"sourceRoot":"","sources":["/home/amirrezatm/Desktop/Amirreza-tm86.github.io-master/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit.prevent=\"authlogin\">\n        <div class=\"mx-auto d-form rounded-2\">\n            <div class=\"AuthForm\">\n                <div class=\"text-center userIcon\">\n                    <font-awesome-icon class=\"fa-2xl\" :icon=\"['fas', 'user']\" />\n                </div>\n                <div class=\"d-Authenticate\">\n                    <div class=\"FormInput\">\n                        <input class=\"input\" v-model=\"store.state.SingupEmail\" type=\"text\" placeholder=\"sign up\"\n                            name=\"signup\" required>\n\n                        <input class=\"input\" v-model=\"store.state.LoginPassword\" type=\"password\" id=\"password\"\n                            placeholder=\"password\" name=\"psw\" required>\n                    </div>\n                </div>\n                <div>\n                    <div class=\"d-showpass mt-2\">\n                        <div>\n                            <input @click=\"showpass\" type=\"checkbox\">\n                        </div>\n                        <div class=\"ms-1\">\n                            <p class=\"text t-showpass\">show password</p>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"authbtn\">SIGN UP</button>\n                </div>\n                <div v-if=\"store.state.ShowErrorLogin\" class=\"error\">\n                    {{ store.state.ErrorAuth }}\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport { useStore } from 'vuex'\nexport default {\n    name: 'signin',\n    data() {\n        const store = useStore();\n\n        console.log(store.state.SingupEmail);\n\n        return {\n            store,\n        }\n    },\n    methods: {\n        submitSiginup: function () {\n\n            const signup = {\n                email: this.store.state.SingupEmail,\n                password: this.store.state.SignupPassword\n            };\n\n            const SignedUp = JSON.parse(sessionStorage.getItem('authenticate'));\n\n            if (SignedUp) {\n\n                this.store.state.ShowErrorSignup = true;\n\n                this.store.state.ErrorSingUp = \"This Email Signed Up!\";\n            } else {\n\n                localStorage.setItem(\"authenticate\", JSON.stringify(signup));\n\n                sessionStorage.setItem(\"authenticate\", JSON.stringify(signup));\n\n                window.location.reload();\n            }\n        },\n        showpass: function () {\n            const inp = document.getElementById('password');\n            if (inp.type === \"password\") {\n                inp.type = \"text\";\n            }\n            else {\n                inp.type = \"password\";\n            };\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}