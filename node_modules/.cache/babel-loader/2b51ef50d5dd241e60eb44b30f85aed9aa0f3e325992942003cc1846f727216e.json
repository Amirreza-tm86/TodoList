{"ast":null,"code":"import { useStore } from \"vuex\";\nexport default {\n  name: \"signin\",\n  data() {\n    const store = useStore();\n    function greet() {\n      const passInp = document.getElementById(\"password\");\n      passInp.classList.add(\"borderBottom\");\n    }\n    return {\n      store,\n      greet\n    };\n  },\n  methods: {\n    submitSiginup: function () {\n      const signup = {\n        email: this.store.state.SingupEmail,\n        password: this.store.state.SignupPassword\n      };\n      const SignedUp = JSON.parse(sessionStorage.getItem(\"authenticate\"));\n      if (SignedUp) {\n        this.store.state.ShowErrorSignup = true;\n        this.store.state.ErrorSingUp = \"This Email Signed Up!\";\n      } else {\n        localStorage.setItem(\"authenticate\", JSON.stringify(signup));\n        sessionStorage.setItem(\"authenticate\", JSON.stringify(signup));\n        window.location.reload();\n      }\n    },\n    showpass: function () {\n      const inp = document.getElementById(\"password\");\n      if (inp.type === \"password\") {\n        inp.type = \"text\";\n      } else {\n        inp.type = \"password\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAqDA,SAASA,QAAO,QAAS,MAAM;AAC/B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACL,MAAMC,KAAI,GAAIH,QAAQ,EAAE;IAExB,SAASI,KAAK,GAAG;MACf,MAAMC,OAAM,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC;MAEnDF,OAAO,CAACG,SAAS,CAACC,GAAG,CAAC,cAAc,CAAC;IACvC;IAEA,OAAO;MACLN,KAAK;MACLC;IACF,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACPC,aAAa,EAAE,YAAY;MACzB,MAAMC,MAAK,GAAI;QACbC,KAAK,EAAE,IAAI,CAACV,KAAK,CAACW,KAAK,CAACC,WAAW;QACnCC,QAAQ,EAAE,IAAI,CAACb,KAAK,CAACW,KAAK,CAACG;MAC7B,CAAC;MAED,MAAMC,QAAO,GAAIC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,cAAc,CAAC,CAAC;MAEnE,IAAIJ,QAAQ,EAAE;QACZ,IAAI,CAACf,KAAK,CAACW,KAAK,CAACS,eAAc,GAAI,IAAI;QAEvC,IAAI,CAACpB,KAAK,CAACW,KAAK,CAACU,WAAU,GAAI,uBAAuB;MACxD,OAAO;QACLC,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEP,IAAI,CAACQ,SAAS,CAACf,MAAM,CAAC,CAAC;QAE5DS,cAAc,CAACK,OAAO,CAAC,cAAc,EAAEP,IAAI,CAACQ,SAAS,CAACf,MAAM,CAAC,CAAC;QAE9DgB,MAAM,CAACC,QAAQ,CAACC,MAAM,EAAE;MAC1B;IACF,CAAC;IACDC,QAAQ,EAAE,YAAY;MACpB,MAAMC,GAAE,GAAI1B,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC;MAC/C,IAAIyB,GAAG,CAACC,IAAG,KAAM,UAAU,EAAE;QAC3BD,GAAG,CAACC,IAAG,GAAI,MAAM;MACnB,OAAO;QACLD,GAAG,CAACC,IAAG,GAAI,UAAU;MACvB;IACF;EACF;AACF,CAAC","names":["useStore","name","data","store","greet","passInp","document","getElementById","classList","add","methods","submitSiginup","signup","email","state","SingupEmail","password","SignupPassword","SignedUp","JSON","parse","sessionStorage","getItem","ShowErrorSignup","ErrorSingUp","localStorage","setItem","stringify","window","location","reload","showpass","inp","type"],"sourceRoot":"","sources":["/home/amirrezatm/Desktop/Amirreza-tm86.github.io-master/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n  <form class=\"formAuth\" @submit.prevent=\"submitSiginup\">\n    <div class=\"mx-auto d-form rounded-2\">\n      <div class=\"AuthForm\">\n        <div class=\"text-center userIcon\">\n          <font-awesome-icon class=\"fa-2xl\" :icon=\"['fas', 'user']\" />\n        </div>\n        <div class=\"d-Authenticate\">\n          <div class=\"FormInput\">\n            <input\n              class=\"input\"\n              v-model=\"store.state.SingupEmail\"\n              @click=\"greet\"\n              type=\"text\"\n              placeholder=\"sign up\"\n              name=\"signup\"\n              required\n            />\n\n            <input\n              class=\"input\"\n              v-model=\"store.state.SignupPassword\"\n              type=\"password\"\n              id=\"password\"\n              placeholder=\"password\"\n              name=\"psw\"\n              required\n            />\n          </div>\n        </div>\n        <div>\n          <div class=\"d-showpass mt-2\">\n            <div>\n              <input @click=\"showpass\" type=\"checkbox\" />\n            </div>\n            <div class=\"ms-1\">\n              <p class=\"text t-showpass\">show password</p>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"clearfix\">\n          <button type=\"submit\" class=\"authbtn\">SIGN UP</button>\n        </div>\n        <div v-if=\"store.state.ShowErrorSignup\" class=\"error\">\n          {{ store.state.ErrorSingUp }}\n        </div>\n      </div>\n    </div>\n  </form>\n</template>\n\n<script>\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"signin\",\n  data() {\n    const store = useStore();\n\n    function greet() {\n      const passInp = document.getElementById(\"password\");\n\n      passInp.classList.add(\"borderBottom\");\n    }\n\n    return {\n      store,\n      greet,\n    };\n  },\n  methods: {\n    submitSiginup: function () {\n      const signup = {\n        email: this.store.state.SingupEmail,\n        password: this.store.state.SignupPassword,\n      };\n\n      const SignedUp = JSON.parse(sessionStorage.getItem(\"authenticate\"));\n\n      if (SignedUp) {\n        this.store.state.ShowErrorSignup = true;\n\n        this.store.state.ErrorSingUp = \"This Email Signed Up!\";\n      } else {\n        localStorage.setItem(\"authenticate\", JSON.stringify(signup));\n\n        sessionStorage.setItem(\"authenticate\", JSON.stringify(signup));\n\n        window.location.reload();\n      }\n    },\n    showpass: function () {\n      const inp = document.getElementById(\"password\");\n      if (inp.type === \"password\") {\n        inp.type = \"text\";\n      } else {\n        inp.type = \"password\";\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n@import url(\"../Style/style.css\");\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}